#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/rgb.h>
#include <dt-bindings/zmk/backlight.h>
#include <dt-bindings/zmk/outputs.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            // ------------------------------------------------------------------------------------------
            // | ESC  |  1  |  2  |  3  |  4  |  5  |  6  |  7  |  8  |  9  |  0  |  -  |  =  |  |  | ~ |
            // | TAB  |  Q  |  W  |  E  |  R  |  T  |  Y  |  U  |  I  |  O  |  P  |  [  |  ]  |  Bksp   |
            // | CTL  |  A  |  S  |  D  |  F  |  G  |  H  |  J  |  K  |  L  |  ;  |  '  |     ENTER     |
            // |  SHIFT     |  Z  |  X  |  C  |  V  |  B  |  N  |  M  |  ,  |  .  | /   |  SHFT   | DEL |
            // |      |  ALT  |  WIN  |            SPACE              | WIN | ALT |     |       |       |
            // ------------------------------------------------------------------------------------------

            bindings = <
  &kp ESC       &kp N1    &kp N2    &kp N3  &kp N4  &kp N5  &kp N6  &kp N7     &kp N8   &kp N9    &kp N0  &kp MINUS  &kp EQUAL  &kp BSLH  &kp GRAVE
  &kp TAB        &kp Q     &kp W     &kp E   &kp R   &kp T   &kp Y   &kp U      &kp I    &kp O     &kp P   &kp LBKT   &kp RBKT  &kp BSPC
&kp LCTRL        &kp A     &kp S     &kp D   &kp F   &kp G   &kp H   &kp J      &kp K    &kp L  &kp SEMI    &kp SQT    &kp RET
&kp LSHFT        &kp Z     &kp X     &kp C   &kp V   &kp B   &kp N   &kp M  &kp COMMA  &kp DOT  &kp FSLH  &kp RSHFT    &kp DEL  &kp LALT   &kp LGUI
           &lt 1 SPACE  &kp RGUI  &kp RALT
            >;

            sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN>;
        };

        raise {
            bindings = <
&kp GRAVE    &kp F1         &kp F2     &kp F3   &kp F4    &kp F5    &kp F6    &kp F7  &kp F8     &kp F9    &kp F10  &kp F11      &kp F12  &trans  &trans
   &trans    &trans  &kp LC(RIGHT)     &trans   &trans    &trans    &trans    &trans  &trans     &trans  &kp PSCRN   &trans       &trans  &reset
   &trans  &kp HOME      &kp PG_DN  &kp PG_UP  &kp END    &trans  &kp LEFT  &kp DOWN  &kp UP  &kp RIGHT     &trans   &trans  &bootloader
   &trans    &trans        &kp DEL     &trans   &trans  &kp LEFT    &trans    &trans  &trans     &trans     &trans   &trans       &trans  &trans  &trans
             &trans         &trans     &trans
            >;

            sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN>;
        };

        Layer__2 {
            bindings = <
&rgb_ug RGB_TOG    &bl BL_TOG        &trans        &trans        &trans        &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
   &out OUT_USB  &out OUT_BLE        &trans        &trans        &trans        &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
     &bt BT_CLR  &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4  &trans  &trans  &trans  &trans  &trans  &trans  &trans
         &trans        &trans        &trans        &trans        &trans        &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
                       &trans        &trans        &trans        &trans        &trans  &trans  &trans  &trans  &trans
            >;
        };
    };
};
